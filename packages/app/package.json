  },
  "dependencies": {
    "@sentry/browser": "^5.13.2",
    "body-scroll-lock": "^3.0.2",
    "chart.js": "^2.9.3",
    "gsap": "^3.2.6",
    "mixpanel-browser": "^2.40.0",
    "nouislider-react": "^3.3.8"
  }
}
