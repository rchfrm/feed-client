{
  "hosting": {
    "site": "deep-links-staging",
    "public": "packages/deep-links/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "function": "instagramRedirect"
      }
    ]
  },
  "functions": {
    "source": "packages/deep-links"
  },
  "emulators": {
    "functions": {
      "port": 5001
    }
  }
}
